def main():
  # Greeting 
  print("Welcome to the Band Name Generator")
  print("====================\n")

#Asking for the users city
city = input("Which city did you grow up in?\n")

#Asking for users pets name
pet = input("\n What's the name of a pet you had or have\n")

# Combining the city and pet name to create a band name
band_name = city + " " + pet 
print(f"\nYour band name is: (band_name)!")

if __name__ == "__main__":
  main()
